#include <bits/stdc++.h>
#define ll long long
#define T ll t; cin>>t; while(t--)
using namespace std;
#define speed ios_base::sync_with_stdio(0)
#define endl "\n"

int  fun(int n,int m)
{
	if(m==1) return 1;
	else if(n==1) return 1;
	else
	{
		return fun(n-1,m)+fun(n,m-1);
	}
}

int main()
{
	speed;
	T
	{
		ll n ,m,i;
		cin>>n>>m;
		cout<<fun(n,m)<<endl;
	}
return 0;
}
