int Solution::lengthOfLastWord(const string q) {
       
        int n = q.length() , c=0 ,i,x=0,flag=0;
        for(i=n-1;i>=0;i--)
        {  
            if(q[i]!=' ')
            break;
        }
        for(;i>=0;i--)
        {
            if(q[i]!=' ')
                c++;
            else
                break;
        }
    return c;
}
